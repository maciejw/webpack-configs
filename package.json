{
  "name": "best-config",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@polymer/lit-element": "^0.6.3",
    "@webcomponents/webcomponentsjs": "^2.2.0",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.3",
    "tslib": "^1.9.3"
  },
  "scripts": {
    "start": "set NODE_ENV=development&& webpack-dev-server --env development --config webpack.config.js",
    "test": "set NODE_ENV=test&& jest",
    "build:analyze": "set NODE_ENV=production&& webpack --env production --analyze --config webpack.config.prod.js",
    "build": "set NODE_ENV=production&& webpack --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "^4.0.3",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.10",
    "@types/react": "16.7.7",
    "@types/react-dom": "16.0.10",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-env": "^1.13.6",
    "bfj": "6.1.1",
    "cache-loader": "^1.2.5",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "1.0.1",
    "dotenv": "6.1.0",
    "dotenv-expand": "4.2.0",
    "fast-async": "^6.3.8",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-custom-media": "^7.0.7",
    "postcss-custom-properties": "^8.0.9",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.4.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.15.2",
    "react-dev-utils": "^6.1.1",
    "react-hot-loader": "^4.3.12",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "thread-loader": "^1.2.0",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "3.1.6",
    "typescript-lit-html-plugin": "^0.9.0",
    "typescript-styled-plugin": "^0.13.0",
    "url-loader": "1.1.2",
    "webpack": "4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "webpackbar": "^3.1.3",
    "workbox-webpack-plugin": "3.6.3"
  }
}
